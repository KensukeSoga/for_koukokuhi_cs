<?xml version="1.0" encoding="UTF-8"?>
<!-- !DOCTYPE SystemParameter SYSTEM "SystemParameter.dtd" -->

<!--Pathの一括変換用の為、仮DTDを内部サブセット定義-->
<!DOCTYPE SystemParameter[
  <!ENTITY xmlpath_win "Isid.KKH.Main.Runtime.">
]>

<SystemParameter>

  <ParameterFile SystemCode="CSFW">
    <SystemName>CSFW</SystemName>
    <ClassName>Isid.NJ.Config.CSParameter</ClassName>
    <Item ParameterName="AppParameter" ItemName="AppForWin" value="&xmlpath_win;CSFWParameter.xml"/>
    <Item ParameterName="ErrorMessage" ItemName="ErrForWin" value="&xmlpath_win;CSFWMessage.xml"/>
  </ParameterFile>

  <ParameterFile SystemCode="HB000000">
    <SystemName>HB000000</SystemName>
    <ClassName>Isid.KKH.Common.KKHUtility.KKHParameter</ClassName>
    <Item ParameterName="AppParameter" ItemName="AppForWin" value="&xmlpath_win;KKHParameter.xml"/>
    <Item ParameterName="ActionMapping" ItemName="ActForWin" value="&xmlpath_win;KKHActionMapping.xml"/>
    <Item ParameterName="ErrorMessage" ItemName ="ErrForWin" value ="&xmlpath_win;KKHMessage.xml" />
    <Item ParameterName="ApplicationId" ItemName ="AppIdForWin" value ="&xmlpath_win;KKHApplicationId.xml" />
  </ParameterFile>

  <ParameterFile SystemCode="InputDataDefi">
    <SystemName>InputDataDefi</SystemName>
    <ClassName>Isid.KKH.Mac.KKHUtility.KKHMacInputDataDefi</ClassName>
    <Item ParameterName="AppParameter" ItemName="AppForWin" value="&xmlpath_win;KKHMacInputDataDefi.xml"/>
  </ParameterFile>

</SystemParameter>
