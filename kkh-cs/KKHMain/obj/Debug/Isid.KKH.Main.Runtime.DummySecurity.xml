	<!--
		ユーザの使用可能なアプリケーション情報の定義。
		各アプリごとにロール情報を設定します。
			id = アプリID
	-->
	<application id="HB000000">

		<!-- itemsの設定可能項目は jp.co.isid.nj.security.Application の定数フィールド参照してください。 -->
		<items>
			<item key="cn" value="d-ruts"/>
			<item key="cedPostState" value="0"/>
			<item key="cedbasedate" value="1"/>
			<item key="cedBusinessCareer" value="0"/>
			<item key="cedActingState" value="0"/>
			<item key="cedOrganizationUnitClass" value="0"/>
			<item key="cedEmergencyMessageOutFlag" value="0"/>
			<item key="cedEmergencyMessage" value=""/>
		</items>

		<!--
			グループの使用可能なアプリケーション情報の定義。
			各ロール種別ごとにロール情報を設定します。
				groupid  = グループNo.
				category = ロール種別
				dataType = 機能区分
				priority = 優先順位
				comment  = コメント（グループ名など）
				roledata = ロール値
		-->

		<!-- d-ratsグループ -->
		<roles groupid="AS-OL000001">
			<!-- 利用可否権限（d-ratsアイコン） -->
			<role category="000" dataType="licensing" priority="010" comment="d-ratsグループ">
				<roledata>11</roledata>
			</role>
		</roles>

		<!-- d-ratsサポートグループ -->
		<roles groupid="AS-OL000002">
			<!-- メニュー利用権限（機能） -->
			<role category="101" dataType="function" priority="100" comment="d-ratsサポートグループ">
				<roledata>11111111111101111111111</roledata>
			</role>
			<!-- バリューパックメニュー利用権限（機能） -->
			<role category="102" dataType="function" priority="100" comment="d-ratsサポートグループ">
				<roledata>1111</roledata>
			</role>
			<!-- 時間区分集計メニュー内利用権限（ラジオボタン） -->
			<role category="111" dataType="function" priority="100" comment="d-ratsサポートグループ">
				<roledata>1</roledata>
			</role>
			<!-- 階層タブ利用権限 -->
			<role category="121" dataType="function" priority="100" comment="d-ratsサポートグループ">
				<roledata>1</roledata>
			</role>
			<!-- 素材画像参照権限 -->
			<role category="131" dataType="function" priority="50" comment="d-ratsサポートグループ">
				<roledata>1</roledata>
			</role>
		</roles>

