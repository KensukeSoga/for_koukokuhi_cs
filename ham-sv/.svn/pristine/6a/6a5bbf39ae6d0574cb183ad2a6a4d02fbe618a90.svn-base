public class SyokuiGrpInfoVO extends AbstractModel{

	private static final long serialVersionUID = 810209213810609793L;

	/**
     * 新規インスタンスを構築します
     */
    public SyokuiGrpInfoVO() {
    }

    /**
     * 新規インスタンスを構築します
     *
     * @return 新規インスタンス
     */
    public Object newInstance() {
        return new SyokuiGrpInfoVO();
    }

    /**
     * グループコードを取得する
     * @return グループコード
     */
