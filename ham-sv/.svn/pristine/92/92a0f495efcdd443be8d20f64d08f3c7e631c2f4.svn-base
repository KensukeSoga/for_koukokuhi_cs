    /** 商品名(サブ） */
    private String _productnmsub = null;

    /** 頻出商品表示数 */
    private int _itemdispnum = 0;


    /**
     * 使用部門コードを取得する
     *
     * @return 使用部門コード
     */
    public String getUsebumoncd() {
        return _usebumoncd;
    }

    /**
     * 使用部門コードを設定する
     *
